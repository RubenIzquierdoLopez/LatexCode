@article{MRUGALAcontactstrcuture,
title = {Contact structure in thermodynamic theory},
journal = {Reports on Mathematical Physics},
volume = {29},
number = {1},
pages = {109-121},
year = {1991},
issn = {0034-4877},
doi = {https://doi.org/10.1016/0034-4877(91)90017-H},
url = {https://www.sciencedirect.com/science/article/pii/003448779190017H},
author = { Mrugala, R. and Nulton, J.D. and {Christian Schön}, J. and  Salamon, P.},
abstract = {We study the contact structure of thermodynamic phase space where the manifold of equilibrium states of any thermodynamic system is represented by some Legendre submanifold. We show that the contact form Θ and its associated covariant derivative allow the construction of a vector field Xƒ corresponding to every function ƒ defined on phase space. Xƒ preserves a Legendre submanifold L, iff ƒ vanishes on L. An Xƒ which preserves L may be interpreted as a process of the system represented by L. Else, Xƒ carries L into the one-parameter family of Legendre submanifols Lt. Such Xƒ generate laws of corresponding states and can be used to obtain information about lesser known systems. Examples of both kinds of Xƒ's are discussed.}
}

@article{MRUGALAcontinuouscontact,
title = {Continuous contact transformations in thermodynamics},
journal = {Reports on Mathematical Physics},
volume = {33},
number = {1},
pages = {149-154},
year = {1993},
issn = {0034-4877},
doi = {https://doi.org/10.1016/0034-4877(93)90050-O},
url = {https://www.sciencedirect.com/science/article/pii/003448779390050O},
author = { Mrugala, R.},
abstract = {We show how the contact structure on the space of thermodynamic parameters M allows the construction of a contact vector field Xƒ corresponding to a thermodynamic function ƒ. The flow associated with such a vector field may be treated as a thermodynamic process if it maps a Legendre manifold, representing a given thermodynamic system, onto itself. In the general case, it maps a Legendre manifold representing one thermodynamic system onto 1-parameter family of Legendre manifolds representing 1-parameter family of another thermodynamic systems. Examples of the contact flows for various functions ƒ are given and briefly discussed.}
}

